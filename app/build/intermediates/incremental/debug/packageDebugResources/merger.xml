tring><string name="advisor_contact_info_title">Advisor Contact Info</string><string name="advisor_name">Name: Dr. John Smith</string><string name="advisor_email">Email: jsmith@ut.edu</string><string name="advisor_office">Office: Plant Hall 123</string><string name="back">Back</string><string name="prompt_first_name">First Name</string><string name="prompt_last_name">Last Name</string><string name="how_to_use_title">How To Use This App</string><string name="how_to_use_intro">Welcome to U-Plan! This application is designed to help you navigate your academic advising journey with ease, providing quick access to FAQs and campus resources.</string><string name="step_1_title">1. Opening the App</string><string name="step_1_text">Locate the U-Plan icon on your home screen and tap it to launch the application.</string><string name="step_2_title">2. Navigating the Main Menu</string><string name="step_2_text">Use the navigation bar at the bottom or the menu icon to switch between Advising Info, FAQs, and Resources.</string><string name="step_3_title">3. Performing the Main Action</string><string name="step_3_text">Tap on any question or resource title to expand the section and view detailed information.</string><string name="step_4_title">4. Saving or Submitting</string><string name="step_4_text">If you fill out any forms or make changes, ensure you tap \'Save\' or \'Submit\' at the bottom of the page.</string><string name="step_5_title">5. Logging Out</string><string name="step_5_text">To exit, you can simply close the app or find the \'Logout\' option in your profile settings.</string><string name="student_profile_title">Student Profile</string><string name="major_label">Major</string><string name="minor_label">Minor</string><string name="graduation_date_label">Graduation Date</string><string name="date_hint">MM/DD/YYYY</string><string name="save_profile_button">Save Profile</string><string name="search_hint">Search questions or resources...</string><string name="faq_title">Frequently Asked Questions</string><string name="faq_1_title">How do I schedule an appointment?</string><string name="faq_1_content">You can schedule an appointment through the main dashboard by clicking the \'Schedule\' button and selecting an available time slot.</string><string name="faq_2_title">Where can I find my student ID?</string><string name="faq_2_content">Your student ID is located on your physical Spartan Card or can be viewed in the \'Student Profile\' section of this app.</string><string name="faq_3_title">How do I change my major?</string><string name="faq_3_content">To change your major, you must meet with your current academic advisor to complete the necessary paperwork and obtain approval.</string><string name="resources_title">Resources</string><string name="resource_1_title">Campus Health Center</string><string name="resource_1_content">The Health Center provides primary care, immunizations, and wellness education. Located near the North Residence Hall.</string><string name="resource_2_title">Career Services</string><string name="resource_2_content">Offers resume help, internship searches, and interview prep. Find them in the Student Center, 2nd floor.</string><string name="resource_3_title">Spartan Support</string><string name="resource_3_content">Provides counseling, financial aid assistance, and academic tutoring resources for all students.</string><string name="expand_collapse_description">Toggle section</string><string name="flowchart_button_description">View Flowchart</string><string name="advisor_button_description">Contact Advisor</string><string name="help_button_description">How to Use</string><string name="faq_button_description">FAQ and Resources</string><string name="profile_button_description">View Profile</string><string name="course_completion_title">Course Completion</string><string name="select_course_label">Select Course</string><string name="select_semester_label">Select Semester</string><string name="add_course_button">ADD COURSE</string><string name="completed_courses_label">Completed Courses</string></file><file name="nav" path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/main/res/navigation/nav.xml" qualifiers="" type="navigation"/><file name="backup_rules" path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/main/res/xml/backup_rules.xml" qualifiers="" type="xml"/><file name="data_extraction_rules" path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/main/res/xml/data_extraction_rules.xml" qualifiers="" type="xml"/><file name="ic_launcher" path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/main/res/mipmap-xhdpi/ic_launcher.webp" qualifiers="xhdpi-v4" type="mipmap"/><file name="ic_launcher_round" path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/main/res/mipmap-xhdpi/ic_launcher_round.webp" qualifiers="xhdpi-v4" type="mipmap"/><file name="ic_launcher" path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/main/res/mipmap-anydpi/ic_launcher.xml" qualifiers="anydpi-v4" type="mipmap"/><file name="ic_launcher_round" path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/main/res/mipmap-anydpi/ic_launcher_round.xml" qualifiers="anydpi-v4" type="mipmap"/></source></dataSet><dataSet aapt-namespace="http://schemas.android.com/apk/res-auto" config="debug$Generated" generated="true" ignore_pattern="!.svn:!.git:!.ds_store:!*.scc:.*:&lt;dir>_*:!CVS:!thumbs.db:!picasa.ini:!*~"><source path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/debug/res"/></dataSet><dataSet aapt-namespace="http://schemas.android.com/apk/res-auto" config="debug" generated-set="debug$Generated" ignore_pattern="!.svn:!.git:!.ds_store:!*.scc:.*:&lt;dir>_*:!CVS:!thumbs.db:!picasa.ini:!*~"><source path="/Users/mvanarsdale/Documents/GitHub/Advising_App_01/app/src/debug/res"/></dataSet><dataSet aapt-namespace="http://schemas.android.com/apk/res-auto" config="generated$Generated" generated="true" ignore_pattern="!.svn:!